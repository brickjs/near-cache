{
  "name": "near-cache-monorepo",
  "version": "1.0.0",
  "description": "Near Cache Monorepo",
  "author": "Vito Limandibhrata <vito.limandibhrata@gmail.com>",
  "homepage": "https://brickjs.io/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brickjs/near-cache.git"
  },
  "keywords": [
    "typescript",
    "reactjs",
    "redis"
  ],
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "commit": "brickjs-commit",
    "bootstrap": "yarn install",
    "prettier:check": "prettier --check \"packages/*/src/**/*.ts\" ",
    "prettier:check:debug": "prettier --debug-check \"packages/*/src/**/*.ts\" ",
    "prettier:write": "prettier --write \"packages/*/src/**/*.ts\" ",
    "eslint": "eslint --fix-dry-run \"./packages/**/*.ts\" ",
    "lerna:build-all": "lerna run build",
    "lerna:test-all": "lerna run test"
  },
  "devDependencies": {
    "@types/jest": "25.2.3",
    "@typescript-eslint/eslint-plugin": "3.4.0",
    "@typescript-eslint/parser": "3.4.0",
    "brickjs-commit": "1.0.0",
    "codecov": "3.7.0",
    "eslint": "7.3.1",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.17.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "lerna": "3.22.1",
    "prettier": "2.0.5"
  }
